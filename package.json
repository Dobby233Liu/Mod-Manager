{
  "name": "doki-doki-mod-manager",
  "productName": "Doki Doki Mod Manager",
  "description": "Mod Manager for Doki Doki Literature Club",
  "author": "u/zuudo",
  "version": "3.2.0",
  "homepage": "https://doki.space",
  "license": "MIT",
  "main": "lib/main/index.js",
  "repository": "https://github.com/DokiDokiModManager/Mod-Manager",
  "bugs": "zudo@doki.space",
  "scripts": {
    "start": "tsc && electron . --enable-sandbox",
    "start-dev": "tsc && electron --inspect=5858 . --enable-sandbox",
    "release": "tsc && electron-builder",
    "test": "tsc && mocha"
  },
  "dependencies": {
    "7zip-bin": "^4.1.0",
    "@fortawesome/fontawesome-free": "^5.5.0",
    "@sentry/electron": "0.15.0",
    "@zudo/unzipper": "^0.0.4",
    "chmodr": "^1.2.0",
    "discord-rich-presence": "^0.0.8",
    "electron-updater": "^4.0.6",
    "file-url": "^2.0.2",
    "fs-extra": "^7.0.1",
    "fuse.js": "^3.4.2",
    "request": "^2.88.0",
    "semver": "^5.6.0",
    "vue": "^2.5.17"
  },
  "devDependencies": {
    "@sentry/cli": "^1.36.4",
    "electron": "^4.0.4",
    "electron-builder": "^20.38.5",
    "mocha": "^5.2.0",
    "typescript": "^3.2.2"
  },
  "build": {
    "appId": "space.doki.modmanager",
    "artifactName": "ddmm-${version}-${os}-${arch}.${ext}",
    "generateUpdatesFilesForAllChannels": true,
    "files": [
      "!**/*.ts",
      "!**/*.map",
      "!**/fontawesome-free/svgs",
      "!**/fontawesome-free/sprites",
      "!**/fontawesome-free/svgs",
      "!**/fontawesome-free/js",
      "!**/fontawesome-free/less",
      "!**/fontawesome-free/scss",
      "!**/vue/src"
    ],
    "appx": {
      "applicationId": "dokidokimodmanager",
      "identityName": "29209Zudo.DokiDokiModManager",
      "publisher": "CN=1FDB3F4F-8BB0-4C8D-B3CE-5046A5C5DFFB",
      "publisherDisplayName": "Zudo",
      "languages": [
        "en-GB"
      ]
    },
    "protocols": [
      {
        "name": "Doki Doki Mod Manager",
        "schemes": [
          "ddmm"
        ]
      }
    ],
    "publish": [
      {
        "provider": "github",
        "repo": "Mod-Manager",
        "owner": "DokiDokiModManager"
      }
    ],
    "win": {
      "target": [
        {
          "target": "nsis",
          "arch": [
            "x64",
            "ia32"
          ]
        },
        {
          "target": "appx",
          "arch": [
            "x64"
          ]
        }
      ]
    },
    "linux": {
      "category": "Game",
      "icon": "build/icon.png",
      "target": [
        {
          "target": "AppImage",
          "arch": [
            "x64",
            "ia32"
          ]
        },
        {
          "target": "zip",
          "arch": [
            "x64",
            "ia32"
          ]
        }
      ]
    },
    "mac": {
      "category": "public.app-category.games",
      "target": [
        "dmg"
      ]
    }
  }
}
